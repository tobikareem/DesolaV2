{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/js/*",
            "/*.{ico,json}"
        ]
    },
    "routes": [
        {
            "route": "/callback",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}